    "import pandas as pd\n",
    "\n",
    "data = input(\"Enter numbers separated by spaces: \")\n",
    "nums = [float(x) for x in data.split()]\n",
    "s = pd.Series(nums)\n",
    "\n",
    "print(\"Mean:\", s.mean())\n",
    "print(\"Standard Deviation:\", s.std())\n"
   ]
